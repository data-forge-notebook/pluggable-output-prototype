  "license": "MIT",
  "dependencies": {
    "apexcharts": "^3.33.0",
    "history": "^5.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "inliner": "^1.13.1",
    "less": "^4.1.2",
    "live-server": "^1.2.1",
    "parcel-bundler": "^1.12.5",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.5.2"
  }
