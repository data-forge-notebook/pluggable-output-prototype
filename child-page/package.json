{
  "name": "react-parcel-typescript-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev": "parcel serve --host=127.0.0.1 --port=5000 index.html",
    "sd": "npm run start:dev",
    "start": "npm run build && live-server out --port=5000 --no-browser",
    "s": "npm run start",
    "build": "parcel build index.html --public-url . --out-dir=dist && inliner dist/index.html > out/index.html",
    "tsc": "tsc",
    "clean": "rm -rf dist out/* .cache"
  },
